  },
  "dependencies": {
    "http": "*",
    "socket.io": "^4.0.0"
  },
  "devDependencies": {
    "prettier": "^2.2.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zshenkman/GameJS"
  }
